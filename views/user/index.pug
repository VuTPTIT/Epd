h1 Users List

form(action="/route/users/create")

  button Add User
  
form(action="/route")

  button Back

form(action="/route/users/search", method="GET")

  input(type="text", name= "q")
    
  button Search
  
ul
  each user in users
    li
      p Name:
        span &nbsp
        span= user.name
        //TẠO KHOẢN CÁCH
      span &nbsp; 
      a(href='/route/users/update/' + user.id) Update
      span &nbsp; 
      a(href='/route/users/delete/' + user.id) Delete